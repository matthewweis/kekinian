#!/bin/sh
SIREUM_HOME=$( cd "$( dirname "$0" )"/.. &> /dev/null && pwd )
classpath() {
  case $(uname -s) in
    CYGWIN*) echo "$(cygpath -C OEM -w -a ${SIREUM_HOME})\\bin\\sireum.jar";;
    *) echo "${SIREUM_HOME}/bin/sireum.jar"
  esac
}
exec java ${JAVA_OPTS} -cp "$( classpath $0 )" org.sireum.Sireum $*
